{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 4
    },
    "cover": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": false
    },
    "beneficiarios": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::beneficiario.beneficiario",
      "mappedBy": "posts"
    }
  }
}
